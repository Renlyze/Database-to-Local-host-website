@model List<CarInsurance.ViewModels.AdminQuoteViewModel>


<h2>Admin View - All Quotes</h2>

<table class="table">
    <tr>
        <th>User ID</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email Address</th>
        <th>Quote</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.FirstName</td>
            <td>@item.LastName</td>
            <td>@item.EmailAddress</td>
            <td>$@item.Quote</td>
        </tr>
    }
</table>
